{
  "type": "sqlite",
  "database": "NadekoBot.db",
  "autoSchemaSync": true,
  "synchronize": false,
  "logging": false,
  "entities": [
     "src/entities/**/*.ts"
  ],
  "migrations": [
     "src/migration/**/*.ts"
  ],
  "subscribers": [
     "src/subscriber/**/*.ts"
  ],
  "cli": {
     "entitiesDir": "src/entities",
     "migrationsDir": "src/migration",
     "subscribersDir": "src/subscriber"
  }
}
