{
  "name": "gvn-bet-bot",
  "version": "1.0.0",
  "description": "Bet bot for GameVN discord",
  "main": "index.js",
  "scripts": {
    "start": "ts-node index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "precommit": "lint-staged && npm run lint",
    "prepush": "npm run git-hook && npm run fixcode",
    "fixcode": "standard --fix",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "lint": "standard --verbose | snazzy",
    "git-hook": "npm run lint -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Doko-Demo-Doa/gvn-bet-bot.git"
  },
  "keywords": [
    "gvn",
    "bet",
    "bot",
    "discord"
  ],
  "author": "Doko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Doko-Demo-Doa/gvn-bet-bot/issues"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "standard --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint",
      "pre-push": "npm run git-hook && npm run fixcode"
    }
  },
  "homepage": "https://github.com/Doko-Demo-Doa/gvn-bet-bot#readme",
  "standard": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  },
  "dependencies": {
    "axios": "^0.19.0",
    "discord.js": "^11.5.1",
    "node-fetch": "^2.6.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^4.0.9",
    "typeorm": "^0.2.18"
  },
  "devDependencies": {
    "@types/node": "^12.6.8",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "chalk": "^2.4.2",
    "dotenv": "^8.0.0",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.1",
    "mocha": "^6.2.0",
    "prettier": "^1.18.2",
    "snazzy": "^8.0.0",
    "standard": "^13.1.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  }
}
